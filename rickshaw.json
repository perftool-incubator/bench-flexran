{
    "rickshaw-benchmark": {
        "schema": {
            "version": "2020.05.18"
        }
    },
    "benchmark": "flexran",
    "controller": {
        "post-script": "%bench-dir%flexran-post-process"
    },
    "client": {
        "files-from-controller": [
            {
                "src": "%bench-dir%/flexran-base",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/flexran-runtime",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/flexran-client",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%flexran-pic-l1",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/flexran-pic-l2",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/flexran-run-pic",
                "dest": "/usr/bin/"
            },
            {
                "src": "%bench-dir%/flexran_env_vars.sh",
                "dest": "/opt/"
            },
            {
                "src": "%bench-dir%/autotest.py",
                "dest": "/usr/bin"
            },
            {
                "src": "%bench-dir%/cpu.py",
                "dest": "/usr/bin"
            },
            {
                "src": "%bench-dir%/log.py",
                "dest": "/usr/bin"
            },
            {
                "src": "%bench-dir%/process_testfile.py",
                "dest": "/usr/bin"
            }
         
        ],
        "runtime": "flexran-runtime",
        "start": "flexran-client"
    }
}
